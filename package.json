{
  "name": "react-serve-monorepo",
  "version": "1.0.0",
  "description": "React-style backend framework for building APIs with JSX",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspace=examples/basic",
    "publish": "npm run build && npm publish --workspaces",
    "test": "npm run test --workspaces"
  },
  "keywords": [
    "react",
    "backend",
    "framework",
    "jsx",
    "api",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
