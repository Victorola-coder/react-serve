    "react-serve",
    "example",
    "auth",
    "prisma"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "react": "^19.1.1",
    "react-serve-js": "^0.5.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/react": "^19.1.10",
    "prisma": "^5.7.0",
    "tsx": "^4.20.4",
    "typescript": "^5.0.0"
  }
}
