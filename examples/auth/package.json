{
  "name": "react-serve-auth-example",
  "version": "1.0.0",
  "description": "Full authentication system example with ReactServe and Prisma",
  "main": "dist/backend.js",
  "scripts": {
    "dev": "tsx --watch backend.tsx",
    "start": "tsx backend.tsx",
    "typecheck": "tsc --noEmit",
